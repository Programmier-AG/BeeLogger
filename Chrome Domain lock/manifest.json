{
    "manifest_version": 2,
  
    "name": "Chrome Domain Locker",
    "description": "",
    "version": "1.0",
  
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "https://ajax.googleapis.com/"
    ]
}